{
  "name": "block-on-block",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "web"
  ],
  "scripts": {
    "generate": "prisma generate",
    "dev:server": "npm --workspace server run dev",
    "dev:web": "npm --workspace web run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "install:all": "npm install && npm --workspace server install && npm --workspace web install"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "express-rate-limit": "^7.5.0",
    "next-auth": "^4.24.11",
    "node-cron": "^3.0.3",
    "react-beautiful-dnd": "^13.1.1",
    "react-hot-toast": "^2.5.2",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "prisma": "^5.10.0"
  }
}
